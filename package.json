{
	"name": "myextension",
	"displayName": "My Extension",
	"description": "test some as beginner",
	"version": "0.0.7",
	"publisher": "ReinRalle",
	"license": "MIT",
	"private": true,
	"icon": "images/Electron.png",
	"keywords": [
		"vscode-extension",
		"example"
	],
	"config": {
		"test1": "test1",
		"test2": "test2"
	},
	"custom": {
		"test": "Hallo Du meine Welt"
	},
	"author": {
		"name": "ReinRalle",
		"email": "reinralle@gmail.com"
	},
	"homepage": "https://github.com/reinralle/myextension#readme",
	"repository": {
		"type": "git",
		"url": "https://github.com/reinralle/myextension.git"
	},
	"bugs": {
		"url": "https://github.com/reinralle/myextension/issues"
	},
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/ext/extension.js",
	"contributes": {
		"commands": [{
				"command": "extension.show_all_vscode_installed_extensions",
				"title": "show all vscode installed extensions (output channel)",
				"category": "myextension"
			},
			{
				"command": "extension.show_all_vscode_commands",
				"title": "show all vscode commands (output channel)",
				"category": "myextension"
			},
			{
				"command": "extension.test_generator",
				"title": "test generator (dbg console)",
				"category": "myextension"
			},
			{
				"command": "extension.getcommands",
				"title": "get commands (dbg console)",
				"category": "myextension"
			},
			{
				"command": "extension.openurl",
				"title": "open url (www.google.com)",
				"category": "myextension"
			}
		]
	},
	"scripts": {
		"build": "tsc -b -v",
		"clean": "tsc -b --clean",
		"compile": "tsc -p ./",
		"postcompile": "terser ./out/ext/extension.js -mc -o ./out/ext/extension.js ",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js",
		"test-inspection": "node --inspect-brk=9229 ./src/test-javascript.js $npm_package_config_test '345 123'",
		"test-bash": "ls -la",
		"precmd-echo": "echo 1. pre",
		"cmd-echo": "echo 2. cmd",
		"postcmd-echo": "echo 3. post",
		"init-weasel": "~/weasel_start.sh",
		"ncu": "ncu",
		"ncu-g": "ncu -g",
		"ncu-u": "ncu -u",
		"npm-check": "npm-check",
		"npm-check-g": "npm-check -g",
		"npm-install": "npm install",
		"npm-list-config-defaults": "npm config list",
		"npm-list-installed-ll": "npm ll --depth 0",
		"npm-list-installed-ls": "npm ls -g --depth 0",
		"npm-list-outdated": "npm outdated",
		"npm-update": "npm update",
		"nvm-ls": ". ~/.nvm/nvm.sh && nvm ls",
		"show_bash_man": "man bash",
		"show-help-bash1": "bash -c help",
		"show-help-bash2": "bash --help",
		"show-help-node": "node -h",
		"show-help-npm": "npm help",
		"show-help-nvm": ". ~/.nvm/nvm.sh; nvm --help",
		"show-help-npx": "npx --help",
		"show-help-terser": "terser -h",
		"show-help-tsc": "npx tsc --help",
		"show-help-tsc-all": "npx tsc --all",
		"show-help-tslint": "tslint -h",
		"show-help-vsce": "vsce --help",
		"show-help-vscode": "\"/mnt/c/Program Files/Microsoft VS Code/bin/code\" --help",
		"show-help-wsl": "wsl.exe --help",
		"show-set": "set",
		"show-variable-config": "echo $npm_package_config_test1",
		"show-variable-custom": "echo $npm_package_custom_test",
		"show-variable-env1": "echo $USER",
		"show-variable-env2": "echo $SHLVL",
		"show-variable-full-package-name": "echo $npm_package_name-$npm_package_version.vsix",
		"lint": "tslint -p ./",
		"vsce-ls": "vsce ls",
		"vsce-show-my-extension": "vsce show reinralle.myextension --json",
		"vsce-package1": "vsce package",
		"vsce-package2": "vsce package --out E:/test.vsix",
		"vscode:prepublish": "npm run compile",
		"watch1": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@actions/core": "^1.2.2",
		"@actions/github": "^2.1.0",
		"@octokit/core": "^2.2.0",
		"@octokit/rest": "^16.40.1",
		"@octokit/routes": "^26.19.1",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^13.5.1",
		"@types/vscode": "^1.41.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"node-jq": "^1.11.0",
		"node-libcurl": "^2.0.3",
		"terser": "^4.6.3",
		"tslint": "^6.0.0",
		"typescript": "^3.7.5",
		"vsce": "^1.71.0",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"colors": "^1.4.0"
	}
}